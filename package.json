{
  "name": "professional-portfolio",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Modern, responsive portfolio website showcasing professional experience, skills, and achievements",
  "author": "Susana Venda",
  "license": "UNLICENSED",
  "keywords": [
    "portfolio",
    "react",
    "vite",
    "resume",
    "professional"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/susanavenda/professional-portfolio.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "echo \"No tests specified\" && exit 0",
    "test:coverage": "echo \"No coverage configured\" && exit 0",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css}\" \"public/**/*.json\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,css}\" \"public/**/*.json\"",
    "clean": "rm -rf dist node_modules/.vite"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "prettier": "^3.8.1",
    "vite": "^6.0.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
